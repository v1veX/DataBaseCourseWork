        let mobile_search = $('.popup__mobile_search');
        if (mobile_search.hasClass('mob-search-show')) {
            mobile_search.removeClass('mob-search-show').hide();
            $('.background__overlay').hide();
        } else {
            mobile_search.addClass('mob-search-show').show();
            $('.mobile__input_search').focus();
            $('.background__overlay').show();
            $('.popup__mobile_phone').removeClass('mob-phone-show').hide();
