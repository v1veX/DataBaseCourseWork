        if (mobile_phone.hasClass('mob-phone-show')) {
            mobile_phone.removeClass('mob-phone-show').hide();
            $('.background__overlay').hide();
        } else {
            mobile_phone.addClass('mob-phone-show').show();
            $('.popup__mobile_search').removeClass('mob-search-show').hide();
            $('.background__overlay').show();
        }
    });
