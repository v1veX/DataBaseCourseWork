function activZacladca() { // проверка есть ли товар в закладках (избранном)
    let name = 'FOREVER';

    let cookie_zac = BX.getCookie(name);
    if (cookie_zac !== undefined) {
        let mas = cookie_zac.split('|');
        mas.forEach(function (item, i, mas) {
            if (item != "") {

                $('.full-quantity-favour').html(
                    i
                );
                $(".add_favour[data-cookieid='" + item + "']").addClass('active');
                // $('.new-like-svg').attr('fill', '#180D03');
            } else {
                // $('.new-like-svg').attr('fill', 'none');
            }
        });
    }
}

var calcView = {

    url: "",
    self:{},
    _init: function () {

        this.self = $(".calc_view");
        this.url = this.self.data('href');

        this.self.click(this.loadView);
    },
    loadView: function () {

        var self = calcView;
        self.url = $(this).data('href');
        $.get(self.url+"?calc_view=y&"+Date.now(),function (data) {
            $("#calcModal").modal();
            $("#calcModal_content").html(data);

        });
        return false;
    }

};
$(function () {
    calcView._init();
});

