        //     if (target > 60) {
        //         $('.catalog_menu').css('top', 0);
        //     } else if (target > 0 && target < 60) {
        //         $('.catalog_menu').css('top', ( 60 - target))
        //     }
        //     if (target == 0) {
        //         $('.catalog_menu').css('top', 60);
        //     }
        // }
    });

});
(function (window){

    // console.log('emitter', BX.Event.EventEmitter.emitter);
    window.OwnGlobalFunctions = function (params) {
        this.favoritesCount = $('.favorites-count_in_circle');
    }

    window.OwnGlobalFunctions.prototype = {
        // this:favoritesCount = $('.favorites-count_in_circle'),
        favoriteSearchAndAddTop: function (action)
        {
            if (action === 'add') {
                let fCount = null;
                let favoritesCount = $('.favorites-count_in_circle');
                if (favoritesCount.html()) {
                    fCount = parseInt(favoritesCount.html()) + 1;
                } else {
                    fCount = 1;
                }
                this.favoritesCssStyles(fCount);
                this.favoritesNewCount(fCount);
            }
            if (action === 'new'){
                this.favoritesSelectAndShowCount();
            }
            if (action === 'delete') {
                let fCount = null;
                let favoritesCount = $('.favorites-count_in_circle');
                console.log('favoritesCount', favoritesCount.text());
                if (favoritesCount.html()) {
                    fCount = parseInt(favoritesCount.html()) - 1;
                } else {
                    fCount = 1;
                }
                // console.log(favoritesCount.text(), 'count', count);
                this.favoritesCssStyles(fCount);
                this.favoritesNewCount(fCount);
            }
        },
        favoritesCssStyles: function (count)
        {
            let favoritesCount = $('.favorites-count_in_circle');
            if (count <= 0 || !count) {
                favoritesCount.hide(20);
            } else {
                favoritesCount.show(250);
                // favoritesCount.css('font-size', '12px');
                // if (count > 9) {
                //     favoritesCount.css('font-size', '11px');
                // }
            }
        },
        favoritesNewCount: function (count)
        {
            let favoritesCount = $('.favorites-count_in_circle');
            if (count > 99) {
                count = 99 + '+';
            }

            favoritesCount.text(count);
        },
        favoritesSelectAndShowCount: function ()
        {
            var param = {
                id: settingSantech.USER_ID
            };
            let that = this;
            $.ajax({
                url:     '/local/templates/new_toptygin/ajax/count_favorites.php', // URL отправки запроса
                type:     "POST",
                dataType: "json",
                data: param,
                success: function(response) { // Если Данные отправлены успешно
                    let res = $.parseJSON(response);
                    // console.log(res, this);
                    that.favoritesCssStyles(res);
                    that.favoritesNewCount(res);

                    return res;
                },
                error: function(jqXHR, textStatus, errorThrown){ // Ошибка
                    console.log('Error: '+ errorThrown);
                }
            });
        },
        /**
         * Добавляем и изменяем значки количества товаров, добавленных в сравнение
         * @param action
         */
        compareSearchAndAddTop: function (action)
        {
            let compareCount = $('.compare-count_in_circle');
            if (action === 'add') {
                let fCount = null;
                if (compareCount.html()) {
                    fCount = parseInt(compareCount.html()) + 1;
                } else {
                    fCount = 1;
                }
                this.compareCssStyles(fCount);
                this.compareNewCount(fCount);
            }
            if (action === 'new'){
                this.compareSelectAndShowCount();
            }
            if (action === 'delete') {
                let fCount = null;
                if (compareCount.html()) {
                    fCount = parseInt(compareCount.html()) - 1;
                } else {
                    fCount = 1;
                }
                // console.log(favoritesCount.text(), 'count', count);
                this.compareCssStyles(fCount);
                this.compareNewCount(fCount);
            }
        },
        compareCssStyles: function (count)
        {
            let compareCount = $('.compare-count_in_circle');
            if (count <= 0 || !count) {
                compareCount.hide(20);
            } else {
                compareCount.show(250);
            }
        },
        compareNewCount: function (count)
        {
            let compareCount = $('.compare-count_in_circle');
            if (count > 99) {
                count = 99 + '+';
            }

            compareCount.text(count);
        },
        compareSelectAndShowCount: function ()
        {
            let param = {
                id: settingSantech.USER_ID
            };
            let that = this;
            $.ajax({
                url:     '/local/templates/new_toptygin/ajax/count_compare.php', // URL отправки запроса
                type:     "POST",
                dataType: "json",
                data: param,
                success: function(response) { // Если Данные отправлены успешно
                    let res = $.parseJSON(response);
                     // console.log('compareSelectAndShowCount', res);
                    that.compareCssStyles(res);
                    that.compareNewCount(res);

                    return res;
                },
                error: function(jqXHR, textStatus, errorThrown){ // Ошибка
                    console.log('Error: '+ errorThrown);
                }
            });
        },
        cartSearchAndAddTop: function (action)
        {
            let cartCount = $('.cart-count_in_circle');
            let compareCount = $('.compare-count_in_circle');
            if (action === 'add') {
                let fCount = null;
                if (compareCount.text()) {
                    fCount = parseInt(cartCount.text()) + 1;
                } else {
                    fCount = 1;
                }
                this.compareCssStyles(fCount);
                this.compareNewCount(fCount);
            }
            if (action === 'new'){
                this.cartSelectAndShowCount();
            }
            if (action === 'delete') {
                let fCount = null;
                if (cartCount.text()) {
                    fCount = parseInt(cartCount.text()) - 1;
                } else {
                    fCount = 1;
                }
                // console.log(favoritesCount.text(), 'count', count);
                this.cartCssStyles(fCount);
                this.cartNewCount(fCount);
            }
        },
        cartCssStyles: function (count)
        {
            let cartCount = $('.cart-count_in_circle');
            if (count <= 0 || !count) {
                cartCount.hide(20);
            } else {
                cartCount.show(250);
            }
        },
        cartNewCount: function (count)
        {
            let cartCount = $('.cart-count_in_circle');
            if (count > 99) {
                count = 99 + '+';
            }
            cartCount.text(count);
        },
        cartSelectAndShowCount: function ()
        {
            let param = {
                id: settingSantech.USER_ID
            };
            let that = this;
            $.ajax({
                url:     '/local/templates/new_toptygin/ajax/count_cart.php', // URL отправки запроса
                type:     "POST",
                dataType: "json",
                async : false,
                data: param,
                success: function(response) { // Если Данные отправлены успешно
                    let res = $.parseJSON(response);
                    // console.log('from item', res);
                    that.cartCssStyles(res);
                    that.cartNewCount(res);
                },
                error: function(jqXHR, textStatus, errorThrown){ // Ошибка
                    console.log('Error: '+ errorThrown);
                }
            });
        },
        //Скрываем | показываем в мобильной версии иконку понравившееся товары
        hideFavoriteMobileHeadIcon:function () {
            // this.favoritesSelectAndShowCount();
            let count = $('#__favorites_count');
            let icon = $('.mobile-icons_favorite');
            var param = 'id=' + settingSantech.USER_ID ;
            let that = this;

            $.ajax({
                url:     '/local/templates/new_toptygin/ajax/count_favorites.php', // URL отправки запроса
                type:     "GET",
                dataType: "html",
                data: param,
                success: function(response) { // Если Данные отправлены успешно
                    let res = $.parseJSON(response);
                    // console.log(res, this);
                    // that.favoritesCssStyles(res);
                    // that.favoritesNewCount(res);
                    if (!res || res <= 0) {
                        icon.hide();
                    } else {
                        icon.show();
                    }

                    return res;
                },
                error: function(jqXHR, textStatus, errorThrown){ // Ошибка
                    console.log('Error: '+ errorThrown);
                }
            });
        },
        //Скрываем | показываем в мобильной версии иконку сранение с количеством
        hideCompareMobileHeadIcon: function () {
            // this.compareSelectAndShowCount()
            //BITRIX_SM_CATALOG_COMPARE_LIST
            let cookie = BX.getCookie('BITRIX_SM_CATALOG_COMPARE_LIST');//BX.parseJSON()
            let count = $('#__compare_count');
            let icon = $('.mobile-icons_compare');
            // console.log('compare count', count.text(), cookie);
            if (!cookie) {
                icon.hide();
            } else {
                icon.show();
            }
        },
        /**
         * Распарсиваем svg и добавляем класс с изменением цвета(или что то ещё)
         * @param svgItem
         * @param svgClass
         */
        svgChangeFill: function (svgItem, svgClass)
        {
            svgItem.each(function() {
                $(this).addClass(svgClass);
                var $img = $(this);
                var imgID = $img.attr('id');
                var imgClass = $img.attr('class');
                var imgURL = $img.attr('src');

                $.get(imgURL, function(data) {
                    // Get the SVG tag, ignore the rest
                    var $svg = $(data).find('svg');

                    // Add replaced image's ID to the new SVG
                    if(typeof imgID !== 'undefined') {
                        $svg = $svg.attr('id', imgID);
                    }
                    // Add replaced image's classes to the new SVG
                    if(typeof imgClass !== 'undefined') {
                        $svg = $svg.attr('class', imgClass);
                    }
                    // Remove any invalid XML tags as per http://validator.w3.org
                    $svg = $svg.removeAttr('xmlns:a');
                    // Replace image with new SVG
                    $img.replaceWith($svg);
                }, 'xml');
            });
        },
        /**
         * Расчитываем стоимость за квадратный метр из ширины и длины и цены
         * @param w
         * @param l
         * @param p
         * @returns {null}
         */
        calculationCostPerSquareMeter: function (w, l, p) {
            let param = {
                width: w,
                length: l,
                price: p,
            };
            let result = null;

            $.ajax({
                url:     '/ajax/calculate_square_meter_price.php', // URL отправки запроса
                type:     "POST",
                dataType: "json",
                async: false,
                data: param,
                success: function(response) { // Если Данные отправлены успешно
                    result = response;
                },
                error: function(jqXHR, textStatus, errorThrown){ // Ошибка
                    console.log('Error: '+ errorThrown);
                    return {'error': true}
                }
            });
            return result;
        },
        //расчет коэффициента измерения для рулонного товара(шаг для калькулятора)
        calculateMeasurementFactorForRoller: function (width) {
            let c = width / 10;

            return parseFloat(c.toFixed(2));
        },
        //
        roundPrice(price)
        {
            return Math.round(price);
        }

    }
})(window);

$(document).ready(function () {
    let OwnGlobalFunctions = new window.OwnGlobalFunctions;

    OwnGlobalFunctions.favoriteSearchAndAddTop('new');
    OwnGlobalFunctions.compareSearchAndAddTop('new');
    OwnGlobalFunctions.cartSearchAndAddTop('new');

    OwnGlobalFunctions.svgChangeFill($('#_top-favorites-svg'), 'svg_hover_class');
    OwnGlobalFunctions.svgChangeFill($('#_top-compare-svg'), 'svg_hover_class');
    OwnGlobalFunctions.svgChangeFill($('#_top-cart-svg'),'svg_hover_class');
    OwnGlobalFunctions.svgChangeFill($('#_top-profile-svg'), 'svg_hover_class');

    OwnGlobalFunctions.hideFavoriteMobileHeadIcon();
    OwnGlobalFunctions.hideCompareMobileHeadIcon();

    // window.OwnGlobalFunctions.favoriteSearchAndAddTop('new');
